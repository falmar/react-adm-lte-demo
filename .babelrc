{
  "presets": [
    ["env", {"modules": false}],
    "stage-2",
    "react"
  ],
  "plugins": ["react-hot-loader/babel"],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "production": {
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-inline-elements"
      ]
    }
  }
}
