  "bugs": {
    "url": "https://github.com/falmar/react-adm-lte-demo/issues"
  },
  "homepage": "https://github.com/falmar/react-adm-lte-demo#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-adm-lte": "0.0.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
