{
  "name": "react-adm-lte-demo",
  "description": "Demo of react-adm-lte",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "eslint src && npm run clean && NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "test": "eslint src && jest",
    "test:watch": "jest --watch --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falmar/react-adm-lte-demo.git"
  },
  "keywords": [
    "react",
    "adminlte"
  ],
  "author": "David Lavieri <daviddlavier@gmail.com> (http://dlavieri.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/falmar/react-adm-lte-demo/issues"
  },
  "homepage": "https://github.com/falmar/react-adm-lte-demo#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-adm-lte": "0.0.8",
    "react-dom": "^15.4.2",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "admin-lte": "^2.3.11",
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.27.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "enzyme"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
